{
  "manifest_version": 2,
  "name": "old project",
  "version": "1.0.0",
  "description": "资源更新",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": ["storage", "*://localhost/*"],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "资源更新",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://localhost/*"],
      "js": ["js/content.js"],
      "run_at": "document_start"
    }
  ]
}
